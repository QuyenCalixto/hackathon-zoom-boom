{
    "manifest_version": 2,
  
    "name": "ZoomBoom",
    "description": "This extension will allow you to spruce up your zoom meetings",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "zoom-boom-icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ],
     "web_accessible_resources": ["zoom-boom-icon.png"]
  }